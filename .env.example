#Passwörter für Import - Default Datum des Import im Format dmY
PW_IMPORT_ELTERN=
PW_IMPORT_AUFNAHME=
PW_IMPORT_MITARBEITER=
PW_IMPORT_VEREIN=

#Copy Logo and Favicon to /public/img and set name here
