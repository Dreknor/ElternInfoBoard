APP_NAME=ElternInfoBoard
#Use production for use and local for testing
APP_ENV=local
# Key is set later
APP_KEY=
#use false in production enviroment
APP_DEBUG=true
# URL for Base-Path
APP_URL=http://localhost

#Setzen, wenn Vertretungsplan aus dem MitarbeiterBoard genutzt wird
