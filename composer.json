{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1 || ^8.2 ",
        "ext-curl": "*",
        "barryvdh/laravel-dompdf": "^2.2",
        "bkwld/cloner": "^3.10",
        "coliving/commentable": "^5.6",
        "danielme85/laravel-log-to-db": "^3.0",
        "devdojo/chatter": "0.2.*",
        "devdojo/laravel-reactions": "^1.0",
        "grosv/laravel-passwordless-login": "^1.10",
        "guzzlehttp/guzzle": "^7.2",
        "laravel-notification-channels/webpush": "^8.0",
        "laravel/framework": "^v11",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "ryangjchandler/blade-cache-directive": "^1.0",
        "sentry/sentry-laravel": "^4.0",
        "socialiteproviders/keycloak": "^5.3",
        "spatie/calendar-links": "^1.4",
        "spatie/icalendar-generator": "^2.3",
        "spatie/laravel-medialibrary": "^11.0",
        "spatie/laravel-permission": "^6.0",
        "spatie/laravel-settings": "^3.4",
        "staudenmeir/eloquent-has-many-deep": "^1.15"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "beyondcode/laravel-dump-server": "^2.0",
        "doctrine/dbal": "^3.0",
        "fakerphp/faker": "^1.9.1",
        "filp/whoops": "^2.9",
        "jasonmccreary/laravel-test-assertions": "^2.0",
        "knuckleswtf/scribe": "^4.31",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^v8.1",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    }
}
